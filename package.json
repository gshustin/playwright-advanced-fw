{
  "name": "playwright-advanced-fw",
  "version": "1.0.0",
  "private": true,
  "scripts": {
  "test": "playwright test",
  "test:headed": "playwright test --headed",
  "test:dev": "cross-env TEST_ENV=dev playwright test --reporter=html",
  "test:stage": "cross-env TEST_ENV=stage playwright test --reporter=html",
  "test:prod": "cross-env TEST_ENV=prod playwright test --reporter=html",
  "report": "echo 'HTML report available in ./playwright-report'",
  "allure:report": "allure generate ./allure-results --clean -o ./allure-report",
  "format": "prettier --write \"**/*.{ts,js,json,md}\""
},
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@types/deep-equal": "^1.0.4",
    "@types/node": "^24.6.2",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^2.15.1",
    "cross-env": "^7.0.3",
    "deep-equal": "^2.2.3",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
